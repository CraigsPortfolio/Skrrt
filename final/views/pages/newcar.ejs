<!DOCTYPE html>
<html lang="zxx">
<head>
  <% include ../partials/head %>
  <% include ../partials/headScripts %>

</head>

<body>
	<!-- Page Preloder -->
	<div id="preloder">
		<div class="loader"></div>
	</div>

	<!-- Header section -->
	<header class="header-section">
		<% include ../partials/profileHeader %>
	</header>
	<!-- Header section end -->

  <!--Calculated Journey -->
  <section id="newcar" class="hero-section set-bg" data-setbg="/img/newcar.jpg">
    <div class="container h-100" id="5">
      <div class="hero-content text-white">
        <div class="row" id="bigRow">
            <h2>New Car</h2>
            <p>Enter the details to add a new car to your garage.</p>

            <!-- inputs for the user details -->
            <form id="newcarform" action="/addcar" method="POST">
              <div class="details-form">
                <input type="text" placeholder="Make" class="input" name="make"><br />
                <input type="text" placeholder="Model" class="input" name="model"><br />
                <input type="text" placeholder="Year" class="input" name="year"><br />
                <input type="text" placeholder="Reg" class="input" name="reg"><br />
                <input type="text" placeholder="Fuel Type" class="input" name="ftype"><br />
                <input type="text" placeholder="Fuel Economy" class="input" name="mpg"><br />
              </div>

            <a href="#nothing" class="site-btn" onClick="document.forms['newcarform'].submit();">Add Car</a>
            <a href="/garage" class="site-btn">Return To Garage</a>
            </form>
            </div>





        </div>
      </div>
  </section>
  <!--Calculated Journey Section End-->





	<!-- Footer section -->
	<footer class="footer-section">
		<% include ../partials/footer %>
	</footer>
	<!-- Footer top section end -->


	<!--====== Javascripts & Jquery ======-->
	<% include ../partials/footerScripts %>

	</body>
</html>
